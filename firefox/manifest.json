{
  "manifest_version": 2,
  "name": "Venus",
  "version": "2.0.1",
  "description": "Replace new tab with a custom tab",
  "browser_action": {
    "default_icon": {
      "48": "images/icon.png"
    },
    "default_title": "Venus Page",
    "default_popup": "main.html"
  },
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "web_accessible_resources": [
    "styles/styles.css",
    "scripts/script.js",
    "scripts/editor.js",
    "images/*",
    "settings/help.html"
  ],
  "icons": {
    "48": "images/icon.png"
  },
  "applications": {
    "gecko": {
      "id": "{63c73c09-8c3c-49af-807c-4997d495343a}"
    }
  }
}
