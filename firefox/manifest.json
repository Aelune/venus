{
  "manifest_version": 2,
  "name": "Venus",
  "version": "0.1.1",
  "description": "Replace new tab with a custom tab",
  "browser_action": {
    "default_icon": {
      "48": "images/icon.png"
    },
    "default_title": "Venus Page",
    "default_popup": "main.html"
  },
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": ["scripts/background.js"],
  "persistent": false
  },
  "web_accessible_resources": [
    "styles/*.css",
    "scripts/script.js",
    "scripts/editor.js",
    "images/*",
    "settings/help.html"
  ],
  "icons": {
    "48": "images/icon.png"
  },
  "applications": {
    "gecko": {
      "id": "{529c968c-bb7f-404b-aa12-34eb5659845d}"
    }
  }
}
